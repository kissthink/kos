#ifndef _mod_list_
#define _mod_list_ 1

#include <list>
using namespace std;
#include "mod_stl_list.h"
#include "mod_allocator.h"

template <typename T>
using InPlaceList = modstd::list<T,InPlaceAllocator<T,0>>;

template <typename T>
using EmbeddedList = modstd::list<T,EmbeddedAllocator<T>>;

template <typename T>
class mod_list_elem {
	char _linkage[sizeof(_List_node<T>)];
public:
	uintptr_t alloc() const { return (uintptr_t)(this); }
};

#endif /* _mod_list_ */
